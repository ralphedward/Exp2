#include <iostream>
#include <conio.h>

using namespace std;

int main()
{
	int r, c, x, y;
	
	cout << "Enter number of rows: "; 
	cin >> r;
	cout << "Enter number of columns: ";
	cin >> c;
	cout << endl;
	
	r = r - 1;
	c = c - 2;
	
	for (x = 0; x <= r; x++)
	{	
		for (y = 0; y <= c; y++)
	{
		cout << "*";
	}
	cout << "*\n";
	}
	
	_getch();
	return 0;
}
